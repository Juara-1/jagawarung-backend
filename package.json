{
  "name": "jagawarung-backend",
  "version": "1.0.0",
  "description": "Backend API for Jagawarung using Node.js, TypeScript, Express, and Supabase",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=unit",
    "test:integration": "jest --testPathPatterns=integration",
    "test:integration:watch": "jest --testPathPatterns=integration --watch",
    "test:integration:coverage": "jest --testPathPatterns=integration --coverage"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "supabase",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-jsdoc-swagger": "^1.8.0"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
