# Environment Variables
PORT=3000
NODE_ENV=development

# Supabase Configuration (Development/Production)
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Supabase Configuration (Testing - Required)
# Required for running integration tests properly and safely
# Create a separate test project in Supabase to avoid affecting production data
SUPABASE_TEST_URL=your_test_supabase_project_url
SUPABASE_TEST_ANON_KEY=your_test_supabase_anon_key
SUPABASE_TEST_SERVICE_ROLE_KEY=your_test_supabase_service_role_key

# AI Provider Configuration
AI_PROVIDER=openai-compatible
AI_BASE_URL=https://api.openai.com/v1
AI_API_KEY=your_ai_api_key
AI_MODEL=gpt-4o-mini
AI_REQUEST_TIMEOUT_MS=30000
